            android:name="com.amap.api.v2.apikey"
            android:value="db412f18e07a2e8945e5ef80f250540b">
            //开发者申请的key</meta-data>

        <activity
            android:name=".activitys.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.SearchedActivity" />
        <activity android:name=".activitys.FangyuanDetail" />
        <activity android:name=".activitys.YuekandingdanActivity" />
        <activity android:name=".activitys.Querenhetong_activity" />
        <activity android:name=".activitys.XinxiquerenActivity" />
        <activity android:name=".activitys.ZufanggongyueActivity" />
        <activity android:name=".activitys.ZizhixinxiActivity" />
        <activity android:name=".activitys.Xinxiqueren2Activity" />
